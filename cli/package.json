{
  "name": "openvp",
  "version": "0.5.1",
  "description": "Command line tool for Open Voluteer Platform",
  "main": "dist/index.js",
  "bin": {
    "openvp": "dist/index.js"
  },
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc",
    "cmdinit": "ts-node ./dist/index.js init test",
    "watch": "tsc -w",
    "lint": "eslint -t stylish --project \"tsconfig.json\"",
    "test": "jest"
  },
  "engines": {
    "node": "14.9.0"
  },
  "devDependencies": {
    "@types/chalk": "0.4.31",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/js-yaml": "4.0.0",
    "@types/node": "12.12.55",
    "@types/node-emoji": "1.8.1",
    "@types/through": "0.0.30",
    "@types/yargs": "16.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.3.0",
    "ncp": "2.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "tsutils": "3.17.1",
    "typescript": "4.1.5",
    "typescript-eslint-parser": "21.0.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "chokidar": "3.4.2",
    "debounce": "1.2.0",
    "execa": "5.0.0",
    "figlet": "1.5.0",
    "glob": "7.1.6",
    "inquirer": "7.3.3",
    "node-emoji": "1.10.0",
    "ora": "5.3.0",
    "parse-github-url": "1.0.2",
    "request": "2.88.2",
    "tar": "6.0.5",
    "tmp": "0.2.1",
    "yargs": "16.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
